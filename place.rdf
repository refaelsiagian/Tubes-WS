        <places:location>Kec. Sirimau, Kota Ambon</places:location>
        <places:discovered>-</places:discovered>
        <places:built>1575</places:built>
        <places:category>Benteng/Kubu/Markas Pertahanan</places:category>
        <places:thumbnail>https://pergiyuk.com/wp-content/uploads/2023/02/Benteng-Nieuw-Victoria.jpg</places:thumbnail>
